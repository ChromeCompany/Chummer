<chummer>
  <metatypes>
  <!--Adds tusks to Orks and Ork Shifters.-->
    <metatype>
      <name>Ork</name>
      <qualities>
        <negative amendoperation="addnode">
          <quality removable="True">Tusks</quality>
        </negative>
      </qualities>
      </metatype>
      <metatype xpathfilter="contains(name,'Shapeshifter')">
        <metavariants>
          <metavariant xpathfilter="name = 'Ork'">
            <qualities>
              <negative amendoperation="addnode">
                <quality removable="True">Tusks</quality>
              </negative>
            </qualities>
          </metavariant>
        </metavariants>
      </metatype>
    
